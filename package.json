{
  "name": "ethproxydelegate",
  "version": "0.0.1",
  "description": "This is an example of a Transparent Upgradeable Proxy pattern written in Solidity and based on @openzeppelin",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "~mr/.cargo/bin/forge build --sizes",
    "prettier:check": "prettier --check \"(contracts|test)/*.sol\"",
    "hint": "solhint contracts/**/*.sol test/**/*.sol",
    "test": "~mr/.cargo/bin/forge test -vvv",
    "clean": "~mr/.cargo/bin/forge clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jac18281828/ethproxydelgate.git"
  },
  "devDependencies": {
    "prettier": "2.8.3",
    "prettier-plugin-solidity": "1.1.1",
    "solhint": "3.3.8"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.8.1"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jac18282828/erthproxydelegate/issues"
  }
}